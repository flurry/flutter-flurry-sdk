<network-security-config>
    <domain-config>
        <domain includeSubdomains="true">data.flurry.com</domain>
        <domain includeSubdomains="true">ads.flurry.com</domain>
        <domain includeSubdomains="true">beap.gemini.com</domain>
        <domain includeSubdomains="true">m.yap.yahoo.com</domain>
        <domain includeSubdomains="true">cfg.flurry.com</domain>
        <domain includeSubdomains="true">proton.flurry.com</domain>
        <trust-anchors>
            <!-- Trust preinstalled CAs -->
            <certificates src="system" />
            <!-- Additionally trust user added CAs -->
            <certificates src="user" />
        </trust-anchors>
    </domain-config>
    <domain-config cleartextTrafficPermitted="true">
        <domain includeSubdomains="true">web21.flurry.bf2.yahoo.com</domain>
        <domain includeSubdomains="true">web21.flurry.gq1.yahoo.com</domain>
        <domain includeSubdomains="true">172.131.222.114</domain>
    </domain-config>
</network-security-config>
